module RndCore:
  "Rock'n'Dice Core"
  version: "0.8"

int Level: 16
  "Уровень"

int exp AttributePoints: 
  "Очки атрибутов"
    @Level - sum(@Attributes.values())

int func MyFunc: 
  "Моя функция"
  int level
  obj attributes
    level - sum(attributes.values())

Attributes:
  "Атрибуты"
    int Organism: 0
      "Организм"
    int Flexibility: 0
      "Гибкость"
    int Intellect: 0
      "Интеллект"
    int Psyche: 0
      "Психика"
